{
  "name": "hacker-news-reader",
  "version": "1.1.0",
  "description": "Hacker News Reader CLI",
  "main": "lib/",
  "scripts": {
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile",
    "lint": "eslint ./src",
    "mocha": "mocha --timeout 15000 --compilers js:babel-register --require babel-polyfill",
    "test": "npm run lint && npm run mocha"
  },
  "keywords": [
    "hacker-news",
    "reader",
    "cli"
  ],
  "bin": {
    "hnr": "./lib/index.js"
  },
  "preferGlobal": true,
  "author": "thomas.blanc.hector@gmail.com",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "openurl": "^1.1.1",
    "ora": "^0.2.1",
    "request": "^2.72.0",
    "vorpal": "^1.10.10",
    "cache-lite": "0.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^6.0.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb-base": "^1.0.4",
    "eslint-plugin-import": "^1.6.0",
    "mocha": "^2.4.5"
  },
  "directories": {
    "test": "test"
  }
}
